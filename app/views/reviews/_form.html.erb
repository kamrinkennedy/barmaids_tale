<%= form_for @review do |f| %>
    <%= f.label :rating, "Rating:(1-5)" %><br>
    <%= f.number_field :rating %><br><br>
    <%= f.label :content, "Comments:" %><br>
    <%= f.text_area :content, size: "30x10" %>
    <%= f.hidden_field :recipe_id%>
    <br>
    <button class="button">Submit Review</button>
<% end %>